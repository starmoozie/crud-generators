{
    "name": "starmoozie/crud-generators",
    "description": "Generate MVC for laravel project",
    "keywords": [
        "generators",
        "model",
        "view",
        "request",
        "config"
    ],
    "homepage": "https://github.com/starmoozie/crud-generators",
    "license": "MIT",
    "authors": [
        {
            "name": "Starmoozie",
            "email": "starmoozie@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "starmoozie/crud": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0||^7.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Starmoozie\\CRUDGenerators\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Starmoozie\\CRUDGenerators\\GeneratorsServiceProvider"
            ]
        }
    }
}
